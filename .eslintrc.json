{
  "extends": [
    "next/core-web-vitals",
    "plugin:prettier/recommended",
    "plugin:@next/next/recommended"
  ],
  "plugins": ["prettier"],
  "globals": {
    "React": "readonly"
  },
  "rules": {
    "no-unused-vars": [
      1,
      {
        "args": "after-used",
        "argsIgnorePattern": "^_"
      }
    ],
    "prettier/prettier": ["warn"]
  }
}
